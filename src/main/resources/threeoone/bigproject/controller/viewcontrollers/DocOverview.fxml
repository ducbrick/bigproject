<AnchorPane fx:id="root" prefHeight="720.0" prefWidth="1200.0" stylesheets="@DocOverviewStyle.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="threeoone.bigproject.controller.viewcontrollers.DocOverviewController">
    <children>
      <GridPane layoutX="157.0" layoutY="72.0" prefHeight="577.0" prefWidth="1022.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="174.0" minWidth="10.0" prefWidth="84.0" />
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="852.0" minWidth="10.0" prefWidth="797.0" />
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="280.0" minWidth="0.0" prefWidth="127.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="528.0" minHeight="10.0" prefHeight="528.0" valignment="BOTTOM" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="101.0" minHeight="10.0" prefHeight="12.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
              <TableView fx:id="table" prefHeight="540.0" prefWidth="1024.0" styleClass="table-view" GridPane.columnIndex="1">
                  <columns>
                      <TableColumn fx:id="number" prefWidth="75.0" styleClass="table-column-header" text="Number" />
                      <TableColumn fx:id="name" prefWidth="135.0" styleClass="table-column-header" text="Name" />
                      <TableColumn fx:id="uploader" prefWidth="150.0" styleClass="table-column-header" text="Uploader" />
                      <TableColumn fx:id="description" prefWidth="663.0" styleClass="table-column-header" text="Description" />
                  </columns>
                  <contextMenu>
                      <ContextMenu fx:id="contextMenu">
                </ContextMenu>
                  </contextMenu>
              </TableView>
              <Button mnemonicParsing="false" onAction="#pressBack" prefHeight="66.0" prefWidth="146.0" styleClass="button" text="BackHome" GridPane.rowIndex="1" />
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="20.0" GridPane.columnIndex="2">
               <children>
                    <Button mnemonicParsing="false" onAction="#pressAddDoc" prefHeight="110.0" prefWidth="116.0" styleClass="button" text="Add New Doc" />
                  <Button mnemonicParsing="false" onAction="#pressSearch" prefWidth="117.0" text="Search" />
               </children>
            </VBox>
         </children>
      </GridPane>
        <Label layoutX="556.0" layoutY="14.0" prefHeight="46.0" prefWidth="224.0" styleClass="label" text="Document Overview">
            <font>
                <Font name="System Bold" size="23.0" />
            </font>
        </Label>
    </children>
