<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox id="menubar" fx:id="box" maxWidth="-Infinity" prefHeight="720.0" prefWidth="78.0" styleClass="pane" stylesheets="@CSS/general.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="threeoone.bigproject.controller.viewcontrollers.MenuBarController">
   <children>
      <Button fx:id="Menu" minWidth="78.0" mnemonicParsing="false" onMouseClicked="#toMenu" prefHeight="54.0" prefWidth="78.0" styleClass="normal-button" text="Home">
         <VBox.margin>
            <Insets top="150.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="AddBook" mnemonicParsing="false" onAction="#toAddBook" onMouseClicked="#toAddBook" prefHeight="54.0" prefWidth="79.0" styleClass="normal-button" text="Add Book">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="DocOverview" mnemonicParsing="false" onAction="#toDocOverview" onMouseClicked="#toDocOverview" prefHeight="54.0" prefWidth="80.0" styleClass="normal-button" text="Browse books" />
      <Button fx:id="AddMember" mnemonicParsing="false" onAction="#toAddMember" prefHeight="58.0" prefWidth="80.0" styleClass="normal-button" text="Add Member" />
      <Button fx:id="MemList" mnemonicParsing="false" onAction="#toMemberList" onMouseClicked="#toMemberList" prefHeight="58.0" prefWidth="80.0" styleClass="normal-button" text="Browse Members" />
   </children>
</VBox>
