<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" prefHeight="720.0" prefWidth="1200.0" stylesheets="@docDetail.css" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="threeoone.bigproject.controller.viewcontrollers.DocumentDetailController">
   <children>
      <ImageView fx:id="cover" fitHeight="191.0" fitWidth="359.0" layoutX="50.0" layoutY="190.0" pickOnBounds="true" preserveRatio="true" />
      <VBox layoutX="454.0" layoutY="203.0" prefHeight="200.0" prefWidth="265.0">
         <children>
            <Label fx:id="bookName" prefHeight="47.0" prefWidth="266.0" text="Name" />
            <Label fx:id="bookDescription" prefHeight="51.0" prefWidth="242.0" text="Description" />
            <Label fx:id="uploader" prefHeight="55.0" prefWidth="266.0" text="Uploader" />
         </children>
      </VBox>
      <Button fx:id="returnToLast" layoutX="50.0" layoutY="100.0" mnemonicParsing="false" onMouseClicked="#returnToLast" text="Return" />
   </children>
</AnchorPane>
